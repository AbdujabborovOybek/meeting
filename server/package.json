    "pm2:unstartup": "pm2 unstartup",
    "pm2:dump": "pm2 dump",
    "pm2:reload": "pm2 reload meet",
    "pm2:reset": "pm2 reset meet",
    "pm2:describe": "pm2 describe meet",
    "pm2:monit": "pm2 monit",
