  "description": "Realtime chat application using Express, Socket.io",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "pm2": "pm2 start server.js --name meet --watch",
    "pm2:stop": "pm2 stop meet",
    "pm2:delete": "pm2 delete meet",
    "pm2:logs": "pm2 logs meet",
    "pm2:status": "pm2 status meet",
    "pm2:restart": "pm2 restart meet",
    "pm2:save": "pm2 save",
